{
  "name": "webstasolutions/concardis-sdk",
  "type":"library",
  "description": "PHP Library for Concardis PayEngine 3.0",
  "keywords":["concardis", "payengine", "payment", "provider", "service", "ecommerce"],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "homepage": "https://www.websta.de/",
  "license": "MIT",
  "authors": [
    {
      "name": "Websta Solutions s.r.o",
      "homepage": "https://www.websta.de/"
    }
  ],
  "require": {
    "php": "~8.0.7",
    "ext-curl": "*",
    "curl/curl": "~2.3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.5",
    "sebastian/version": "3.0.2"
  },
  "autoload": {
    "psr-4": {
      "Concardis\\Payengine\\Lib\\": "src/Concardis/Payengine/Lib"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Concardis\\Payengine\\Lib\\Test\\": "tests/Concardis/Payengine/Lib/Test"
    }
  }
}
